* Save users
./bin/django dumpdata auth.user > user.json
# remove internal user from user.json

* Reset DB & media
rm -rf var/media/*
./bin/django flush --noinput
./bin/django migrate --fake

* Load fixtures
./bin/django loaddata user.json
./bin/django loaddata .salt/files/cg44/fixtures.json
cp .salt/files/cg44/upload/* var/media/upload/

* Load MNT/communes
./bin/django loaddem
./bin/django import -v2 parsers.CG44CityParser COMMUNE-44.shp
